{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
		"components.d.ts",
		"auto-imports.d.ts",
		"typed-router.d.ts"
	],
  
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
